using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace if_elseIniciante01
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int n1 = 3;
            int n2 = 2;

            bool n1MaiorN2 = n1 > n2;
            bool n1MenorN2 = n1 < n2;
            bool n1IgualN2 = (n1 == n2);

            if (n1 > n2)
            {
                Console.WriteLine("N1 é maior do que N2!"); 
            }
            else if (n1IgualN2)
            {
                Console.WriteLine("N1 é igual ao N2!");
            }
            else 
            {
                Console.WriteLine("N1 é menor do que N2!");
            }

            Console.ReadKey();

        }
    }
}
